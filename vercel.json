{
    "version": 2,
    "builds": [
        {
        "src": "public/**/*",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/socket.io",
        "dest": "../server.js"
        },
        {
        "src": "/(.*)",
        "dest": "/index.html"
        }
    ]
}
  